[alias]
	l = log --pretty=oneline -n 20 --graph
	s = status -sd
	d = diff --patch-with-stat
	undopush = push -f origin HEAD^:master
[apply]
	whitespace = fix
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[github]
        user = gaelenh
[merge]
	log = true
